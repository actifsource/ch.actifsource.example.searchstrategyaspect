<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="ch.actifsource.core.RegisterModel">
      <ResourceFolder
            path="asrc">
      </ResourceFolder>
   </extension>
   <extension
         point="ch.actifsource.environment.RegisterBuiltin">
      <Builtin
            defaultBuiltin="true"
            isExtendable="true"
            name="MODELSNIPPET">
         <Plugin
               pluginId="ch.actifsource.example.searchstrategyaspect">
         </Plugin>
      </Builtin>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <command
               commandId="ch.actifsource.example.searchstrategyaspect.HierarchyCommandSearch"
               label="Hirarchy Search Test"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <iterate
                        operator="and">
                     <adapt
                           type="ch.actifsource.core.PackagedResource">
                        <test
                              property="ch.actifsource.environment.preferences.typeOf"
                              value="84759c2c-98ee-11ea-8ef1-cb1cd2be9da3">
                        </test>
                     </adapt>
                  </iterate>
                  <count
                        value="1">
                  </count>
               </with>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <command
               commandId="ch.actifsource.example.searchstrategyaspect.SimpleCommandSearch"
               label="Simple Search Test"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <iterate
                        operator="and">
                     <adapt
                           type="ch.actifsource.core.PackagedResource">
                        <test
                              property="ch.actifsource.environment.preferences.typeOf"
                              value="84759c2c-98ee-11ea-8ef1-cb1cd2be9da3">
                        </test>
                     </adapt>
                  </iterate>
                  <count
                        value="1">
                  </count>
               </with>
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="ch.actifsource.example.searchstrategyaspect.hierarchy.HierarchySearchCommand"
            id="ch.actifsource.example.searchstrategyaspect.HierarchyCommandSearch"
            name="HierarchyCommandSearch">
      </command>
      <command
            defaultHandler="ch.actifsource.example.searchstrategyaspect.simple.SimpleSearchCommand"
            id="ch.actifsource.example.searchstrategyaspect.SimpleCommandSearch"
            name="SimpleCommandSearch">
      </command>
   </extension>

</plugin>
